{
    "functions": {
      "api/server.js": {
        "memory": 1024,
        "includeFiles": "db.json"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "api/server.js"
      }
    ],
    "builds": [
        { "src": "api/server.js", "use": "@vercel/node" }
      ],
      "routes": [
        { "src": "/player", "dest": "/api/player" }
  }